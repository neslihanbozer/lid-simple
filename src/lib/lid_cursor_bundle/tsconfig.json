{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ],
      "@/src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "data"
  ]
}